FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

setFormat csv;

surfaceFormat null;

interpolationScheme cellPoint;

fields
(
    U1
    U2
    U1Mean
    U2Mean
	k1
	k2
	k1Mean
	k2Mean
    Theta
    ThetaMean
    v1
	v1Mean
    v2
	v2Mean
);

sets
(
    profile
    {
        type            uniform;
		nPoints			200;
        axis            y;
        start           (9 0 0.0025);
        end             (9 0.1 0.0025);
    }

);

