FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      v1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 2.2e-4;

boundaryField
{
    frontAndBackPlanes
    {
        type            empty;
    }
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    lowerWall
    {
        type            fixedValue;
        value           uniform 1e-12;
    }
    outlet
    {
        type            inletOutlet;
        phi             phi1;
        inletValue      uniform 0;
        value           $internalField;
    }
    upperWall
    {
        type            fixedValue;
        value           uniform 1e-12;
    }
}


// ************************************************************************* //
