FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0";
    object      U1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.36 0 0);

boundaryField
{
    frontAndBackPlanes
    {
        type            empty;
    }
    inlet
    {
        type            fixedValue;
        value           uniform (0.36 0 0);
    }
    lowerWall
    {
        type            zeroGradient;
    }
    outlet
    {
        type            pressureInletOutletVelocity;
        phi             phi1;
        value           uniform (0.36 0 0);
    }
    upperWall
    {
        type            zeroGradient;
    }
}


